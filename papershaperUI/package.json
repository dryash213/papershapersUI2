    "axios": "^1.7.9",
    "eslint": "^8.57.1",
    "firebase": "^11.0.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.1",
    "react-markdown": "^10.0.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1",
